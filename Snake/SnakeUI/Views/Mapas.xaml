<Page
    x:Class="SnakeUI.Mapas"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SnakeUI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:vm="using:SnakeUI.ViewModels"
    mc:Ignorable="d">

    <Page.DataContext>
        <vm:MapasViewModel></vm:MapasViewModel>
    </Page.DataContext>

    <Grid Name="GridPrincipal" Background="#c5cae9">       
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>

        <!--Panel Superior de la vista-->
        <StackPanel Name="PanelPrincipal" Grid.ColumnSpan="2" >
            <TextBlock Name="NombreVista"  Text="Mapas" FontSize="50dp" Margin="20" TextAlignment="Center" FontFamily="Assets/3Dumb.ttf#3Dumb"></TextBlock>
            <!--Panel de Botones-->
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Margin="3">
                <Button Name="BotonJugar" Content="Jugar" Click="BotonJugar_Click" Margin="2" FontFamily="Assets/3Dumb.ttf#3Dumb" />
                <Button Name="MapasTopCincuenta" Content="TopMapas" Margin="2" FontFamily="Assets/3Dumb.ttf#3Dumb" />
                <Button Name="TodosLosMapas" Content="AllMapas" Margin="2" FontFamily="Assets/3Dumb.ttf#3Dumb" />
            </StackPanel>
        </StackPanel>


        <!--Listado de mapas-->
        <ScrollViewer Grid.Column="1" Grid.Row="1"   >
            <ListView Name="ListadoMapas" Margin="5" Background="Transparent"  ItemsSource="{Binding ListaMapas}"  SelectedItem="{Binding MapaSeleccionado,Mode=TwoWay}">
                <ListView.ItemTemplate>
                    <DataTemplate >
                        <!--Datos de Mapas-->
                        <RelativePanel Padding="5" Margin="10" BorderBrush="Black" BorderThickness="3" MinWidth="400">
                            <TextBlock Name="txtBlck_NombreMapa" Text="{Binding NombreMapa}" Margin="7" FontFamily="Assets/3Dumb.ttf#3Dumb"/>
                            <TextBlock Name="txtBlck_PuntuacionMapa" Text="{Binding ValoracionMapa}" Margin="5" RelativePanel.Below="txtBlck_NombreMapa" FontFamily="Assets/3Dumb.ttf#3Dumb"/>
                            <TextBlock Name="txtBlck_CreadorMapa" Text="{Binding NombreUsuario}" Margin="5" RelativePanel.RightOf="txtBlck_NombreMapa" RelativePanel.AlignBottomWith="txtBlck_NombreMapa" FontFamily="Assets/3Dumb.ttf#3Dumb" />
                        </RelativePanel>
                    </DataTemplate>
                </ListView.ItemTemplate>
            </ListView>
        </ScrollViewer>
        
        <!--Previsualizacion del mapa-->
        <RelativePanel Name="relativeFondo" Background="#836fa9" Margin="10" Grid.Column="0" 
                       Grid.Row="1" RelativePanel.AlignHorizontalCenterWithPanel="True" 
                       RelativePanel.AlignVerticalCenterWithPanel="True" >
           
